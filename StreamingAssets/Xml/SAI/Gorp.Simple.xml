<?xml version="1.0" encoding="utf-8" ?>
<gorp>
  <algorithms>
    <algorithm namespace="Simple.JailBreak">
      <input model="orp"/>
      <output model="escape"/>
      <function>
        <expressions>
          <expression sequence="0">
            <Less input="orp" result="escape.result">
              <parameters>
                <parameter position="0">
                  <pathvariable type="int" namespace="orp.timbucktu.sunrise"/>
                </parameter>
                <parameter position ="1">
                  <variable type="int" namespace="orp.limitofcheese" isconstant="true"/>
                </parameter>
              </parameters>
            </Less>
          </expression>
        </expressions>
      </function>
    </algorithm>
    <algorithm namespace="Simple">
      <input model="orp"/>
      <output model="escape"/>
      <function>
        <expressions>
          <expression sequence="0">
            <Algorithm namespace="Simple.JailBreak" input="orp" output="escape" />
          </expression>
        </expressions>
      </function>
    </algorithm>
  </algorithms>
  <models>
    <model namespace="orp">
      <variables>
        <variable name="limitofcheese" type="int" isimmutable="true">500</variable>
        <variable name="harvey" type="int">17</variable>
        <variable name="irrelevant" type="string">"More Than Five"</variable>
      </variables>
      <pathvariables>
        <pathvariable path="timbucktu" name="sunrise" type="int">89</pathvariable>
        <pathvariable path="timbucktu" name="sunset" type="int">-22</pathvariable>
        <pathvariable path="timbucktu" name="tanzania" type="key">9</pathvariable>
      </pathvariables>
      <arrays>
        <array path="local" name="hillgiant" type="[int]">11, 17, 4</array>
      </arrays>
    </model>
    <model namespace="escape">
      <variables>
        <variable name="result" type="bool">false</variable>
      </variables>
    </model>
  </models>
</gorp>
